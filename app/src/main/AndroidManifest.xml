<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.divinelimoutah.divinelimousine">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ReservationActivity"
            android:label="@string/title_activity_reservation" />
        <activity
            android:name=".ReserveCarActivity"
            android:label="@string/title_activity_reserve_car" />
        <activity
            android:name=".PreferencesActivity"
            android:label="@string/title_activity_preferences"></activity>
    </application>

</manifest>
